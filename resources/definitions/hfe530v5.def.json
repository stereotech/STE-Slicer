      "value": "resolveOrValue('printing_mode') in ['classic']"
    },
    "machine_shape": {
      "default_value": "elliptic",
      "value": "'rectangular' if resolveOrValue('printing_mode') in ['classic'] else 'elliptic'"
    },
    "machine_five_axis": {
      "default_value": true
    },
    "machine_start_gcode": {
      "default_value": ";STE Slicer 5D Gcode\nSTART\nG1 F200 E15 ;extrude 15 mm of feed stock\nG1 F200 E13.5 ;make retraction\nG92 E0 ;zero the extruded length again\n{coordinate_system};Move to the first coordinate system"
    },
    "machine_middle_gcode": {
      "default_value": "G91\nG1 F200 E-4\nG0 Z2\nG90\nG54\nG0 X0 Y20\nG92 E0 C0\n{cylindrical_rotate}\n{coordinate_system}\nG1 F200 E6\nG92 E0"
    },
    "machine_end_gcode": {
      "default_value": "END"
    },
    "machine_extruder_count": {
      "default_value": 2
    },
    "machine_disallowed_areas": {
      "default_value": [
        [
          [
            -157.5,
            150
          ],
          [
            -157.5,
            -150
          ],
          [
            -152.5,
            -150
          ],
          [
            -152.5,
            150
          ]
        ]
      ],
      "value": "[[[-157.5, 150],[ -157.5, -150],[ -152.5, -150],[-152.5, 150]]] if resolveOrValue('machine_shape') == 'rectangular' else []"
    }
  }
}