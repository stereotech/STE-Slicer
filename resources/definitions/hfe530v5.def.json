{
  "version": 2,
  "name": "Stereotech Fiber 530 V5",
  "inherits": "steprinter_v5",
  "metadata": {
    "author": "Stereotech",
    "manufacturer": "Stereotech",
    "visible": true,
    "file_formats": "text/x-gcode",
    "has_materials": true,
    "machine_extruder_trains": {
      "0": "stereotech_v5_extruder_main",
      "1": "stereotech_v5_fiber_extruder"
    },
    "first_start_actions": [],
    "supported_actions": []
  },
  "overrides": {
    "machine_name": {
      "default_value": "Stereotech Fiber 530 V5"
    },
    "machine_width": {
      "default_value": 315
    },
    "machine_height": {
      "default_value": 300,
      "value": "300 if resolveOrValue('printing_mode') in ['classic'] else 250"
    },
    "machine_depth": {
      "default_value": 300
    },
    "machine_hybrid": {
      "default_value": true
    },
    "machine_center_is_zero": {
